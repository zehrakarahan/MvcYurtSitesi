    @section scripts{
        <script type="text/javascript">
        var pageNo = 1;
        $(window).scroll(function () {
            if ($(window).scrollTop() == $(document).height() - $(window).height()) {
                $.ajax({
                    url: '/Anasayfa/liste/',
                    data: { page: pageNo },
                    success: function (data) {
                        $("#ajax-main").append(data);
                        pageNo++;
                    }

                });
            }
        });
        </script>
    }
